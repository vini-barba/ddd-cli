{
  "name": "generate-ddd-cli",
  "version": "0.0.1",
  "description": "a cli to generate ddd components",
  "main": "bin/generate-ddd-cli.js",
  "author": "Barba<vinicius_gomes.94@hotmail.com>",
  "license": "MIT",
  "preferGlobal": true,
  "bin": {
    "generate-ddd-cli": "bin/generate-ddd-cli.js"
  },
  "scripts": {
    "test": "jest",
    "start": "ts-node ./src/index.ts",
    "build": "rm -rf ./dist && tsc && cp -r ./src/templates/ ./dist/src/templates",
    "local": "yarn build && node ./bin/generate-ddd-cli.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint -c .eslintrc.js \"src/**/*.ts\" ",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vini-barba/generate-ddd-cli"
  },
  "keywords": [
    "cli",
    "ddd",
    "generate",
    "generator",
    "generate-ddd-cli"
  ],
  "files": [
    "bin",
    "dist/**/*"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.4",
    "@semantic-release/npm": "^9.0.1",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.173",
    "@swc/jest": "^0.2.20",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "semantic-release": "^19.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "commander": "^9.2.0",
    "inquirer": "^8.2.4"
  }
}
